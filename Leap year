class Program
{
    static void Main(string[] args)
    {
        int a; 

        Console.WriteLine("Enter a:");
        a = int.Parse(Console.ReadLine());
        if (a%4==0 & a%100!=0 || a%400==0)
        {
            Console.WriteLine("It is a leap year");
        }
        else
        {
            Console.WriteLine("It is not a leap year");
        }
    }
}
